{
  "name": "stubby-front-end",
  "version": "0.0.1",
  "description": "stubby-front-end - front-end start-kit on stack: gulp, webpack, babel, reactjs, stylus, mocha",
  "author": "Mikhail Makhotkin",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "set-tools": "npm run git-hooks && npm run ln-scaffolder",
    "ln-scaffolder": "ln -sf ../../scripts/bin/scaffolder.js $(npm bin)/scaffolder",
    "git-hooks": "env VERBOSE=true ./scripts/bin/init-git-hooks.sh",
    "test": "mocha --opts \"mocha.opts\"",
    "autotest": "npm --loglevel=silent test -- --watch",
    "release": "env NODE_ENV=production gulp release"
  },

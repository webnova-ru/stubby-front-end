    "dateformat": "^1.0.11",
    "del": "^1.2.0",
    "event-stream": "^3.3.1",
    "extend": "^2.0.1",
    "fs-extra": "^0.24.0",
    "gulp": "^3.9.0",
    "gulp-base64": "^0.1.3",
    "gulp-changed": "^1.2.1",
    "gulp-conflict": "^0.4.0",
    "gulp-consolidate": "^0.1.2",
